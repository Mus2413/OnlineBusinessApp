<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:showIn="@layout/app_bar_home"
    android:padding="10dp">
    tools:context=".Admin.AdminCategoryActivity">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/scrollview">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">
        <LinearLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:id="@+id/linear1_category_layout"
    android:orientation="horizontal"
    android:layout_centerHorizontal="true"
    android:gravity="center"
    android:padding="20dp">
    <androidx.cardview.widget.CardView
        android:layout_width="100dp"
        android:layout_height="150dp"
        android:id="@+id/cardview1"
        app:cardCornerRadius="4dp"
        app:cardElevation="3dp">

        <RelativeLayout
            android:layout_width="100dp"
            android:layout_height="150dp">
            <ImageView
                android:id="@+id/daalimage"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:src="@drawable/daal"
                android:elevation="3dp"
                android:scaleType="centerCrop"
                android:focusableInTouchMode="true"/>
            <TextView
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:text="Pulses"
                android:textColor="@color/colorblack"
                android:textSize="15dp"
                android:textStyle="bold"
                android:layout_below="@id/daalimage"
                android:textAlignment="center"
                android:layout_marginTop="5dp"></TextView>
        </RelativeLayout>
    </androidx.cardview.widget.CardView>
    <androidx.cardview.widget.CardView
        android:layout_width="100dp"
        android:layout_height="150dp"
        android:id="@+id/cardview2"
        app:cardCornerRadius="4dp"
        app:cardElevation="3dp"
        android:layout_marginLeft="15dp">

        <RelativeLayout
            android:layout_width="100dp"
            android:layout_height="150dp">
            <ImageView
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:src="@drawable/rices"
                android:id="@+id/riceimage"
                android:scaleType="centerCrop"
                android:elevation="3dp"/>
            <TextView
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:text="Rice"
                android:textColor="@color/colorblack"
                android:textSize="15dp"
                android:textStyle="bold"
                android:layout_below="@id/riceimage"
                android:textAlignment="center"
                android:layout_marginTop="5dp"></TextView>
        </RelativeLayout>
    </androidx.cardview.widget.CardView>
    <androidx.cardview.widget.CardView
        android:layout_width="100dp"
        android:layout_height="150dp"
        android:id="@+id/cardview3"
        app:cardCornerRadius="4dp"
        app:cardElevation="3dp"
        android:layout_marginStart="15dp">
        <RelativeLayout
            android:layout_width="100dp"
            android:layout_height="150dp">
            <ImageView
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:src="@drawable/masala"
                android:scaleType="centerCrop"
                android:id="@+id/masalaimage" />
            <TextView
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:text="Spices"
                android:textColor="@color/colorblack"
                android:textSize="15dp"
                android:textStyle="bold"
                android:layout_below="@id/masalaimage"
                android:textAlignment="center"
                android:layout_marginTop="5dp"></TextView>

        </RelativeLayout>
    </androidx.cardview.widget.CardView>
</LinearLayout>


